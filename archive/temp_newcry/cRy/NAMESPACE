exportMethods("computeReciprocalUnitCell","computeUnitCell")

# BravaisType and Lattice
exportClasses("BravaisType","Lattice")
exportMethods("getCentringOps")                               # Applied to both Lattice and Symmetry

# Generic functions with associated methods
exportMethods("isCorrect","getParameters","getData","getFields")

# Standard methods
exportMethods("print","[")

